@article{Danielsson963119,
   author = {Danielsson, Nanette and Norell-Clarke, Annika and Hagquist, Curt},
   institution = {Karlstad University},
   institution = {Karlstad University, Centre for Research on Child and Adolescent Mental Health},
   institution = {Karlstad University, Department of Health Sciences},
   journal = {Sleep Health},
   number = {3},
   pages = {194--197},
   title = {Associations between adolescent sleep disturbance and different worry themes: findings from a repeated cross-sectional study from 1988 to 2011},
   volume = {2},
   DOI = {10.1016/j.sleh.2016.05.005},
   keywords = {Sleep disturbance, Adolescence, Worry, Time trends, Sex},
   abstract = {Objective The objective was to investigate relationships between adolescent sleep disturbance and various worry themes. Methods Questionnaire data from 8 cross-sectional collections between 1988 and 2011 were used. The sample included more than 20,000 adolescents aged 15-16. Binary logistic regressions were used for the analyses. Results Sleep disturbance and female sex increased the odds of worrying about all themes. Sleep disturbance shared stronger associations with worry about financial security, accidents/illness, being bullied, and terrorist attacks (odds ratios, 2.65-3.35) compared with worry about environmental destruction or nuclear war (odds ratios, 1.73-2.11). No interactions between sleep and year of investigation were found. Conclusions Little is known about the association between adolescent worry and sleep, and about sleep disturbance and specific worry content. This study shows that the strength in the relationship between adolescent worry and sleep varies with worry themes. Knowledge of the worry content related to sleep may aid in targeting preventions and interventions. },
   year = {2016}
}
