   institution = {Karlstad University, Department of Mathematics},
   journal = {Bulletin of the London Mathematical Society},
   pages = {957--967},
   title = {The discriminant 10 Shimura curve and its associated Heun functions},
   volume = {48},
   DOI = {10.1112/blms/bdw055},
   keywords = {Shimura curves, modular forms, Heun functions, Picard-Fuchs equations, exceptional sets, CM periods},
   abstract = {The Shimura curve of discriminant 10 is uniformized by a subgroup of an arithmetic $(2,2,2,3)$ quadrilateral group. We derive the differential structure of the ring of modular forms for the Shimura curve and relate the ring generators to explicit Heun functions for the quadrilateral group. We also show that the Picard–Fuchs equation of the associated family of abelian surfaces has solutions that are modular forms. These results are used to completely describe the exceptional sets of the Heun functions, and we show how to find examples like \[ Hl\left(\frac{27}{2},\frac{7}{36}; \frac{1}{12},\frac{7}{12},\frac{2}{3},\frac{1}{2}; -\frac{96}{25}\right)=\frac{2^{1/2}5^{2/3}}{3^{4/3}}. \] },
   year = {2016}
}
