@article{Karlsson1064018,
   author = {Karlsson, Michael},
   institution = {Karlstad University, Faculty of Arts and Social Sciences (starting 2013)},
   journal = {OBS - Observatorio},
   number = {4},
   pages = {150--165},
   title = {Goodbye politics, hello lifestyle : Changing news topics in tabloid, quality and local newspaper websites in the U.K. and Sweden from 2002 to 2012},
   volume = {10},
   keywords = {tabloidization, digitalization, online news, content analysis},
   abstract = {Although considerable efforts have studied online news, studies so far have not investigated how the actual news topics are affected by digitalization in general, if at all, or compared them to different media constructs. Instead, changes in content are assumed or illustrated anecdotally rather than systematically assessed. This empirical study, covering Swedish and UK news sites within tabloid, quality morning, and local/regional varieties between 2002 and 2012, shows that there is a tabloidization effect in general but that it is stronger in tabloids and in Sweden compared to the UK. Further, this tabloidization can be more precisely described as a shift from political to more lifestyle journalism, as it is in the areas where the prime growth and decline are found. In addition, the study reveals that it is the slower news that increases most suggesting that the immediate character of online news is mediated by production conditions },
   year = {2016}
}
